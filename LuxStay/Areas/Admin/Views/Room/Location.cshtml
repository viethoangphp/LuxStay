@model IEnumerable<LuxStay.Areas.Admin.Data.LocationModel>
<div class="col-md-6">
    <div class="form-group">
        <label>Khu Vực</label>
        <select class="form-control select2 select2-blue" data-dropdown-css-class="select2-danger"
                style="width: 100%;">
            @foreach(var item in Model)
            {
                <option value="@item.id">@item.LocationName</option>
            }
        </select>
    </div>
</div>
