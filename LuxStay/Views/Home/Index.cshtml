                    </path>
                </svg>
            </a>
        </div>
        <div class="header__search-box">
            <div class="header__search-box--wrapper is-flex middle">
                <!-- header input search -->
                <div class="header__search--item header__search-input">
                    <i class="header__search-icon fas fa-search"></i>
                    <input type="text" placeholder="Tìm kiếm">
                </div>
                <!-- header input days -->
                <div class="header__search--item header__search-day">
                    <div class="is-flex middle">
                        <i class="header__search-icon far fa-calendar"></i>
                        <input type="text" name="daterange" class="app-datepicker datepicker__header" readonly />
                    </div>
                </div>
                <!-- header input people -->
                <div class="header__search--item header__search-people has-dropdown-box">
                    <i class="header__search-icon fas fa-user"></i>
                    <p>Số khách</p>
                    <div class="header__search-people--counter dropdown-box">
                        <div class="select-number">
                            <div class="select-number__title">
                                <p>Người lớn</p>
                                <span>Trên 18 tuổi</span>
                            </div>
                            <div class="select-number__input-wp">
                                <div class="select-number__input--decrease disable">
                                    -
                                </div>
                                <div class="select-number__input">
                                    <input type="text" placeholder="0">
                                </div>
                                <div class="select-number__input--increase ">
                                    +
                                </div>
                            </div>
                        </div>
                        <div class="select-number">
                            <div class="select-number__title">
                                <p>Trẻ em</p>
                                <span>Tuổi từ 2-12</span>
                            </div>
                            <div class="select-number__input-wp">
                                <div class="select-number__input--decrease">
                                    -
                                </div>
                                <div class="select-number__input">
                                    <input type="text" placeholder="0">
                                </div>
                                <div class="select-number__input--increase ">
                                    +
                                </div>
                            </div>
                        </div>
                        <div class="select-number">
                            <div class="select-number__title">
                                <p>Trẻ sơ sinh</p>
                                <span>Dưới 2 tuổi</span>
                            </div>
                            <div class="select-number__input-wp">
                                <div class="select-number__input--decrease">
                                    -
                                </div>
                                <div class="select-number__input">
                                    <input type="text" placeholder="0">
                                </div>
                                <div class="select-number__input--increase ">
                                    +
                                </div>
                            </div>
                        </div>
                        <div class="select-number__options is-flex">
                            <button class="select-number__options--delete">Xóa</button>
                            <button class="select-number__options--save">Áp dụng</button>
                        </div>
                    </div>
                </div>
                <!-- header search btn -->
                <div class="header__search--item btn header__search-btn p-0">
                    <i class="header__search-icon fas fa-search"></i>
                </div>
            </div>
        </div>

    </div>
    <nav class="header__navbar col l-6 is-flex middle">
        <ul class="header__navbar--list is-flex middle">
            @using LuxStay.Areas.Admin.Data
            @if (Session["USER"] == null)
            {
                <!-- Not login -->
                <li class="header__navbar--item header__navbar-register">
                    <a href="/Account/Register">Đăng ký</a>
                </li>
                <li class="header__navbar--item header__navbar-login">
                    <a href="/Account/Login">Đăng nhập</a>
                </li>
            }
            else
            {
                <!-- Login -->
                <li class="header__navbar--item header__navbar-user has-dropdown-box">
                    <a class="is-flex middle btn--dropdown">
                        <div class="user-avar">
                            <img src="~/Assets/Client/assets/img/user-ava.jpg" alt="">
                        </div>
                        <span>@(((CustomerModel)Session["USER"]).fullname)</span>
                    </a>
                    <div class="user-menu dropdown-box">
                        <ul class="user-menu__list">
                            <li class="user-menu__item">
                                <a href="" class="user-menu__link">
                                    <i class="far fa-calendar"></i>
                                    <p>Đặt chỗ của tôi</p>
                                </a>
                            </li>
                            <li class="user-menu__item">
                                <a href="" class="user-menu__link">
                                    <i class="fas fa-cog"></i>
                                    <p>Cài đặt tài khoản</p>
                                </a>
                            </li>
                            <li class="user-menu__item">
                                <a href="" class="user-menu__link">
                                    <i class="far fa-heart"></i>
                                    <p>Yêu thích</p>
                                </a>
                            </li>
                            <li class="user-menu__item">
                                <a href="/Account/Logout" class="user-menu__link">
                                    <i class="fas fa-sign-out-alt"></i>
                                    <p>Đăng xuất</p>
                                </a>
                            </li>
                            <li class="user-menu__line"></li>
                        </ul>
                    </div>
                </li>
            }
            <!-- crurrency -->
            <li class="header__navbar--item header__navbar-currency has-dropdown-box">
                <a class="is-flex middle btn--dropdown">
                    <div class="currency-avar">
                        <img src="~/Assets/Client/assets/img/flag/vn.svg" alt="">
                    </div>
                    <span>VND</span>
                </a>
                <div class="currency-box dropdown-box">
                    <div class="is-flex">
                        <div class="currency-box__language ">
                            <a href="" class="is-flex middle is-checked">
                                <img src="~/Assets/Client/assets/img/flag/vn.svg" alt="">
                                <span>Tiếng Việt</span>
                            </a>
                            <a href="" class="is-flex middle ">
                                <img src="~/Assets/Client/assets/img/flag/en.svg" alt="">
                                <span>English</span>
                            </a>
                        </div>
                        <div class="currency-box__monney">
                            <a href="" class="is-flex middle is-checked">
                                <b>VND</b>
                                <span>Việt Nam Đồng</span>
                            </a>
                            <a href="" class="is-flex middle ">
                                <b>USA</b>
                                <span>United States Dollar</span>
                            </a>
                        </div>
                    </div>
                </div>
            </li>
        </ul>
    </nav>
}
